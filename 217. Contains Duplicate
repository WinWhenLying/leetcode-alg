1 19ms 54.55%

/* 
 * 利用一个set
 */

class Solution {
    public boolean containsDuplicate(int[] nums) {
        HashSet<Integer> st = new HashSet<>();
        for(int n:nums) {
        	if(st.contains(n)) {
        		return true;
        	} else {
        		st.add(n);
        	}
        }
        return false;
    }
}

